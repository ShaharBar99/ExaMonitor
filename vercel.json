{
  "version": 2,
  "builds": [
    {
      "src": "examonitor-vite/package.json",
      "use": "@vercel/vite"
    },
    {
      "src": "server/src/app.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/src/app.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
